<script>
function clearsearchbox(){
			$("#serachico").attr("src","/img/search2.gif");
			$("#ajaxsearchbox").hide();
}
function serchajax (){ 
		if ($("#search").val().length > 1){
				$("#searchform").addClass("bgwait");
				$.ajax({url: "/ajax/search_ajax.php",type:'POST',cache: false,data:{searchword:$("#search").val()},
					success: function(html){if (html !="0"){$("#ajaxsearchbox").html(html).fadeIn();
					$("#searchform").removeClass("bgwait");	}else clearsearchbox();},
					error: function(){clearsearchbox();	}
				});
			}
			else {clearsearchbox();}
			
}
function plusweb_adaptive()
{
	if ($(window).width() < 1000){
		$("#mainblock").addClass('mbfull');
		$(".pcstyle").addClass('mobilestyle').removeClass('pcstyle');
		
		$(".pconly").hide();
		$(".mobileonly").show();
		if($("#anonsmobile").html() =="" )
		{$("#anonsmobile").html($("#anonspc").html());
		$("#anonspc").html("");}
	}
	else {
		$("#mainblock").removeClass('mbfull');
		$(".mobilestyle").addClass('pcstyle').removeClass('mobilestyle');
		$(".mobileonly").hide();
		$(".pconly").css("visibility","visible").show();
		if($("#anonspc").html() =="" ){
		$("#anonspc").html($("#anonsmobile").html());
		$("#mobile").html("");}
	
	}	
	$(".mobilestyle #smoothmenu1").hide();
	$(".pcstyle #smoothmenu1 ").show();
	//closesearch();
	//leftpanelclose();
	
	
	
}
	
$(function(){
plusweb_adaptive();
$(window).resize(function(){plusweb_adaptive();leftpanelclose()});
$("#searchpanel").hover(function(){serchajax();},function(){ clearsearchbox();})				
$("#search").keyup(function(event){if (event.keyCode != 40) if (event.keyCode != 38)  serchajax();});
	$(".kdwn").live("keydown", function(event){
		if (event.keyCode == 40) 
		{
			var ind= parseFloat($(this).attr("tabindex"));
			ind++;
			$('.kdwn').removeClass("hover");
			$('.kdwn').removeClass("hover");
			$('.kdwn[tabindex='+ind+']').addClass("hover").focus();
			return false ;
		}
		if (event.keyCode == 38) 
		{
			var ind= parseFloat($(this).attr("tabindex"));
			ind--;
			$('.kdwn').removeClass("hover");
			if (ind>1)$('.kdwn[tabindex='+ind+']').addClass("hover").focus();
			$('.kdwn[tabindex='+ind+']').focus();
			return false ;
		}
	})	
	$(".addfav").live("click", function(){
		var clk=$(this);
		if(clk.hasClass("added")) {
			
		
		clk.addClass("weit");
			$.ajax({url: "/ajax/addfav_ajax.php",type:'POST',cache: false,data:{"delfavor":clk.attr("rel")},
					success: function(html){/*alert(html); */clk.removeClass("weit"); 	clk.removeClass("added"); clk.attr('title','Добавить в избранное');
					
					}, 
					error: function(){clk.removeClass("weit"); }
			})
		}
		else {
			clk.addClass("weit");
			$.ajax({url: "/ajax/addfav_ajax.php",type:'POST',cache: false,data:{"addfavor":clk.attr("rel")},
					success: function(html){/*alert(html);*/clk.removeClass("weit");clk.addClass("added"); clk.attr('title','Убрать из избранного');
					$("#favorites").removeClass("hidden");}, 
					error: function(){clk.removeClass("weit"); }
			})
			
		}
	})
})
</script>

