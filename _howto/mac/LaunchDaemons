brew cask install launchcontrol - устанавливаем графический интерфейс для запуска скриптов
$ brew search launchcontrol
==> Casks
launchcontrol ✔

(env) imac-jocker:selenium jocker$ launchctl unload /home/jocker/Library/LaunchAgents/my.a223.tests.plist
(env) imac-jocker:selenium jocker$ launchctl load /home/jocker/Library/LaunchAgents/my.a223.tests.plist
(env) imac-jocker:selenium jocker$ launchctl list|grep a223
-	0	my.a223.tests



$ sudo launchctl load -w /System/Library/LaunchDaemons/ssh.plist


    /System/Library/LaunchDaemons for native macOS processes
    /Library/LaunchDaemons for installed third-party apps



    /Library/LaunchAgents for all user accounts
    ~/Library/LaunchAgents for a specific user account
    /System/Library/LaunchAgents for macOS only


launchctl list
launchctl remove name
launchctl remove com.annoying.service

launchctl unload -w /System/Library/LaunchDaemons/com.apple.photolibraryd


# ----------------------------------
# Демо для запуска питонячьего файла
# из ~/Library/LaunchAgents/
# каждую минуту
# ----------------------------------
(env) imac-jocker:selenium jocker$ launchctl load /home/jocker/Library/LaunchAgents/my.a223.tests.plist
(env) imac-jocker:selenium jocker$ launchctl unload /home/jocker/Library/LaunchAgents/my.a223.tests.plist
(env) imac-jocker:selenium jocker$ cat /home/jocker/Library/LaunchAgents/my.a223.tests.plist

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Disabled</key>
    <false/>
    <key>EnvironmentVariables</key>
    <dict>
	<key>PATH</key>
	<string>/Users/jocker/selenium/env/bin</string>
	<key>PYTHONPATH</key>
	<string>/Users/jocker/selenium/env/lib/python3.6/site-packages</string>
    </dict>
    <key>Label</key>
    <string>my.a223.tests</string>
    <key>Nice</key>
    <integer>1</integer>
    <key>ProgramArguments</key>
    <array>
	<string>/Users/jocker/selenium/env/bin/pytest</string>
	<string>--cache-clear</string>
	<string>/Users/jocker/selenium/a223.py</string>
	<string>-s</string>
    </array>
    <key>StandardErrorPath</key>
    <string>/Users/jocker/my_a223_err.txt</string>
    <key>StandardOutPath</key>
    <string>/Users/jocker/my_a223_out.txt</string>
    <key>StartInterval</key>
    <integer>2500</integer>
</dict>
</plist>



